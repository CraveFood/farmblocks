{
  "name": "@crave/farmblocks-alert",
  "version": "0.1.0",
  "description": "A React Component for displaying alert bar messages",
  "main": "index.js",
  "scripts": {
    "release": "npm publish ./dist/npm --access=public",
    "copy-npm-files":
      "copy AUTHORS LICENSE README.md CHANGELOG.md package.json dist/npm/.",
    "transpile": "babel ./src/ -d ./dist/npm/",
    "build": "yarn transpile && yarn copy-npm-files",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "babel": {
    "presets": ["flow", "es2040", "react"],
    "env": {
      "test": {
        "presets": ["es2015", "react"]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CraveFood/farmblocks.git"
  },
  "keywords": [
    "farmblocks",
    "react",
    "component",
    "alert",
    "alertbar",
    "widget"
  ],
  "author": "Crave Food Services and AUTHORS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CraveFood/farmblocks/issues"
  },
  "homepage": "https://github.com/CraveFood/farmblocks#readme",
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.0.0-rc.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.10",
    "@storybook/addon-links": "^3.2.10",
    "@storybook/addon-storyshots": "^3.2.10",
    "@storybook/cli": "^3.2.10",
    "@storybook/react": "^3.2.10",
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2040": "^1.1.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "copy": "^0.3.1",
    "flow-bin": "^0.55.0",
    "jest": "^21.1.0",
    "react": "^16.0.0-rc.3",
    "react-dom": "^16.0.0-rc.3",
    "react-test-renderer": "^16.0.0-rc.3",
    "regenerator-runtime": "^0.11.0"
  }
}
